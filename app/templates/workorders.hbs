<div>
    <div class="card" id="WorkorderCard">
      <div class="row card-content center">
  			<b><span class="col s8 card-title" id="paddingLeft">{{name}}</span></b>
        <span class="col s2" id="Username">{{username}}</span>
        <span class="col s2">
          <div class="center-align push-down">
      								{{#link-to 'index'}}<button type="submit" class="btn waves-effect white-text red lighten-2">Logout</button>{{/link-to}}
      		</div>
        </span>
  		</div>
    </div>
    <div>
      <b><span class="text-weight">Work Orders</span></b>
    </div>
    <div>
      <table class="striped">
        <thead>
          <tr>
              <th id="source">Source</th>
              <th id="Product">Product</th>
              <th>Original Quantity</th>
              <th>Current Quantity</th>
              <th>Due Date</th>
          </tr>
        </thead>
        <tbody>
          {{#each model as |rental|}}
          {{#link-to value}}
          <tr class="red lighten-1">
            <td>{{rental.source}}</td>
            <td>
              {{rental.product}}
            </td>
            <td>{{rental.originalquantity}}</td>
            <td>{{rental.currentquantity}}</td>
            <td>{{rental.date}}</td>
          </tr>
          {{/link-to}}
          {{/each}}
        </tbody>
      </table>
    </div>
</div>
